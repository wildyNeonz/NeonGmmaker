{
	"source": {
		"include": "./src/webdocs/gmmd.ts",
		"exclude": "node_modules"
	},
	"plugins": [
		"@webdoc/plugin-markdown",
		"@webdoc/plugin-format"
	],
	"template": {
		"readme": "./README.md",
		"siteRoot": "NeonGmmaker/",
		"outputSourceFiles": false,
		"applicationName": "Neon GMMAKER",
		"title": "NGMMaker docs",
		"meta": {
			"og:title": "NGMMaker docs",
			"og:description": "NGMMaker in-game API documentation"
		}
	},
	"manifest": {
		"root": "NeonGmmaker/docs"
	},
	"opts": {
		"destination": "docs",
		"export": "./docs/webdoc.api.json"
	}
}